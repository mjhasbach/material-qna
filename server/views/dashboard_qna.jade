block dashboard_qna
    div(ng-if='setting.selected === "QnA"', ng-controller='dashboardQnAController', layout='column', flex)
        md-tabs.qna(md-selected='tabs.i', md-stretch-tabs='always', flex)
            include dashboard_qna_add_edit
            include dashboard_qna_search
            include dashboard_qna_upload
            include dashboard_qna_image